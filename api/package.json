{
  "name": "api",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "main": "lib/server.js",
  "scripts": {
    "start": "node .",
    "start:watch": "nodemon .",
    "lint": "eslint .",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "container": "run-s container:build container:start",
    "container:build": "docker build . -t tagcon-api-container",
    "container:start": "docker run -p 3000:3000 tagcon-api-container",
    "container:dev": "docker-compose up --build",
    "container:test": "docker-compose -f docker-compose.test.yml up --build"
  },
  "dependencies": {
    "config-yml": "^0.10.3",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "mongoose": "^5.9.19",
    "morgan": "~1.9.1"
  },
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@babel/preset-env": "^7.10.2",
    "babel-cli": "^6.26.0",
    "babel-jest": "^26.0.1",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-plugin-import": "^2.20.2",
    "jest": "^26.0.1",
    "nodemon": "^2.0.4",
    "npm-run-all": "^4.1.5",
    "superagent": "^5.2.2",
    "supertest": "^4.0.2",
    "webpack": "^4.43.0"
  }
}
